{
    "metadata": [{
      "src": {
        "src": "../",
        "files": "src/*.csproj",
        "exclude": [
          "src/bin/**",
          "src/obj/**",
          "src/.paket/**",
          "src/paket-files/**"
        ]
      },
      "properties": {
        "TargetFramework": "netstandard2.0"
      },
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }],
    "build": {
      "globalMetadata": {
        "_appTitle": "Axle Framework Website",
        "_appFooter": "Copyright &copy; Virtual Dimensions 2013 - 2020. All rights reserved.",
        "_enableSearch": true,
        "newFileRepository": {
          "branch": "master"
        },
        "_gitContribute": {
          "repo": "https://github.com/vdimensions/axle/",
          "branch": "master",
          "path": "codebase/resources/properties/docfx/api_spec"
        },
        "_gitUrlPattern": "github"
      },
      "content": [
        {
          "files": [
            "api/**.yml",
            "api/index.md"
          ]
        },
        {
          "files": [
            "articles/**.md",
            "articles/**/toc.yml",
            "toc.yml",
            "*.md"
          ]
        }
      ],
      "xrefService": [
        "https://xref.docs.microsoft.com/query?uid={uid}"
      ],
      "resource": [
        {
          "files": [
            "images/**"
          ]
        }
      ],
      "overwrite": [
        {
          "files": [
            "apidoc/**.md"
          ],
          "exclude": [
            "obj/**",
            "_site/**"
          ]
        }
      ],
      "dest": "_site",
      "globalMetadataFiles": [],
      "fileMetadataFiles": [],
      "template": [
        "default",
        "../../../docs/templates/site"
      ],
      "postProcessors": [],
      "markdownEngineName": "markdig",
      "noLangKeyword": false,
      "keepFileLink": false,
      "cleanupCacheHistory": true,
      "disableGitFeatures": false
    }
  }