  "build": {
    "template": [
      "default",
      "templates/site"
    ],
    "content": [
      {
        "files": [
          "**/*.yml"
        ],
        "src": "obj/docfx/api",
        "dest": "api"
      },
      {
        "files": [ 
          "*.md",
          "toc.yml",
          "api/toc.yml",
          "api/**.md",
          "api_spec/**",
          "articles/**",
          "roadmap/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "api_spec/**.md"
        ],
        "exclude": [
          "obj/**"
        ]
      }
    ],
    "pdf": {
      "template": [
        "pdf.default",
        "templates/pdf"
      ],
      "content": [
        {
          "files": [
            "api/**.yml",
          ],
          "exclude": [
            "**/toc.yml",
            "**/toc.md"
          ]
        },
        {
          "files": [
            "articles/**.md",
            "articles/**/toc.yml",
            "toc.yml",
            "*.md",
            "pdf/*"
          ],
          "exclude": [
            "**/bin/**",
            "**/obj/**",
            "_site_pdf/**",
            "**/toc.yml",
            "**/toc.md"
          ]
        },
        {
          "files": "pdf/toc.yml"
        }
      ],
      "resource": [
        {
          "files": [
            "images/**"
          ],
          "exclude": [
            "**/bin/**",
            "**/obj/**",
            "_site_pdf/**"
          ]
        }
      ],
      "overwrite": [
        {
          "files": [
            "apidoc/**.md"
          ],
          "exclude": [
            "**/bin/**",
            "**/obj/**",
            "_site_pdf/**"
          ]
        }
      ],
      "dest": "_site_pdf"
    },
    "globalMetadata": {
      "_appTitle": "Axle Framework Website",
      "_appFooter": "Copyright &copy; Virtual Dimensions 2013 - 2019. All rights reserved.",
      "_enableSearch": true,
      "newFileRepository": {
        "branch": "master"
      },
      "_gitContribute": {
        "repo": "https://github.com/vdimensions/axle-framework",
        "branch": "master",
        "path": "docs/api_spec"
      },
      "_gitUrlPattern": "github"
    },
    "markdownEngineName": "markdig",
    "dest": "../_site",
    "xrefService": [ "https://xref.docs.microsoft.com/query?uid={uid}" ]
  }
}
